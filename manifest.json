{
    "name": "trial",
    "version": "1.0",
    "description": "Chrome URL Blocking Extension",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup" : "popout.html"
    },
  
    "manifest_version": 2
  }
  